#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/macluiggy/Documents/PROGRAMMING/MERN/movie-reviews/node_modules/.pnpm/mime@1.6.0/node_modules/mime/node_modules:/home/macluiggy/Documents/PROGRAMMING/MERN/movie-reviews/node_modules/.pnpm/mime@1.6.0/node_modules:/home/macluiggy/Documents/PROGRAMMING/MERN/movie-reviews/node_modules/.pnpm/node_modules:/home/macluiggy/Documents/PROGRAMMING/MERN/movie-reviews/node_modules:/home/macluiggy/Documents/PROGRAMMING/MERN/node_modules:/home/macluiggy/Documents/PROGRAMMING/node_modules:/home/macluiggy/Documents/node_modules:/home/macluiggy/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/macluiggy/Documents/PROGRAMMING/MERN/movie-reviews/node_modules/.pnpm/mime@1.6.0/node_modules/mime/node_modules:/home/macluiggy/Documents/PROGRAMMING/MERN/movie-reviews/node_modules/.pnpm/mime@1.6.0/node_modules:/home/macluiggy/Documents/PROGRAMMING/MERN/movie-reviews/node_modules/.pnpm/node_modules:/home/macluiggy/Documents/PROGRAMMING/MERN/movie-reviews/node_modules:/home/macluiggy/Documents/PROGRAMMING/MERN/node_modules:/home/macluiggy/Documents/PROGRAMMING/node_modules:/home/macluiggy/Documents/node_modules:/home/macluiggy/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
